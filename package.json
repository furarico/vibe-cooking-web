{
  "name": "vibe-cooking",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "next lint --fix && prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "api:generate": "openapi-typescript ./openapi/openapi.yaml -o ./src/types/api.d.ts",
    "api:preview": "redocly preview-docs openapi/openapi.yaml",
    "db:dev": "dotenv -e .env.development -- prisma",
    "db:prod": "dotenv -e .env.production -- prisma",
    "db:generate": "prisma generate",
    "db:migrate:dev": "dotenv -e .env.development -- prisma migrate dev",
    "db:migrate:prod": "dotenv -e .env.production -- prisma migrate deploy",
    "db:studio:dev": "dotenv -e .env.development -- prisma studio",
    "db:studio:prod": "dotenv -e .env.production -- prisma studio",
    "db:reset:dev": "dotenv -e .env.development -- prisma migrate reset",
    "db:reset:prod": "dotenv -e .env.production -- prisma migrate reset",
    "deploy:gcr": "gcloud run deploy vibe-cooking-web --source . --region asia-northeast1 --project furarico-vibe-cooking",
    "predev": "pnpm api:generate && pnpm db:generate",
    "prebuild": "pnpm api:generate && pnpm db:generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@firebase/app-check": "^0.10.0",
    "@google-cloud/cloud-sql-connector": "^1.8.1",
    "@google/genai": "^1.5.1",
    "@next/third-parties": "^15.3.4",
    "@prisma/client": "^6.9.0",
    "@radix-ui/react-progress": "^1.1.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "firebase": "^11.9.1",
    "firebase-admin": "^13.4.0",
    "lucide-react": "^0.514.0",
    "next": "15.3.3",
    "next-themes": "^0.4.6",
    "openapi-fetch": "^0.14.0",
    "prisma": "^6.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@redocly/cli": "^1.34.3",
    "@storybook/addon-docs": "^9.0.8",
    "@storybook/addon-onboarding": "^9.0.8",
    "@storybook/nextjs": "^9.0.8",
    "@tailwindcss/postcss": "^4.1.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.0",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.28.0",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-storybook": "^9.0.8",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "openapi-typescript": "^7.8.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "storybook": "^9.0.8",
    "tailwindcss": "^4.1.8",
    "tsx": "^4.20.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": "22.x",
    "pnpm": "10.x"
  }
}
